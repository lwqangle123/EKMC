X=X';
y=y';

for j=1:32
x_train = X(1:10000,:);
y_train = y(1:10000,j);
x_test = X(10001:11000,:);
y_test = y(10001:11000,j);
fit2 = cvglmnet(x_train, y_train, 'binomial',[],[],10)
Pred = glmnetPredict(fit2.glmnet_fit, x_train)

Pred=double(Pred>0.5);
mae(j)=min(sum(abs(Pred-y_test))/size(Pred,1));
end
